{
  "name": "dance-studio-back",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "_moduleAliases": {
    "@authModule": "dist/modules/auth",
    "@classesModule": "dist/modules/classes",
    "@hallsModule": "dist/modules/halls",
    "@paymentsModule": "dist/modules/payments",
    "@pricesModule": "dist/modules/prices",
    "@registrationsModule": "dist/modules/registrations",
    "@rolesModule": "dist/modules/roles",
    "@schedulesModule": "dist/modules/schedules",
    "@usersModule": "dist/modules/users",
    "@decorators": "dist/core/decorators",
    "@guards": "dist/core/guards",
    "@middlewares": "dist/core/middlewares",
    "@services": "dist/core/services",
    "@pipes": "dist/core/pipes",
    "@core": "dist/core",
    "@root": "dist"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.json",
    "test:e2e:watch": "jest --config ./jest-e2e.json --watch"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "create-nestjs-middleware-module": "^0.2.2",
    "dotenv": "^16.0.1",
    "module-alias": "^2.2.2",
    "mysql2": "^2.3.3",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.19.0",
    "sequelize-typescript": "^2.1.3",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^16.0.0",
    "@types/sequelize": "^4.28.12",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "sequelize-cli": "^6.4.1",
    "sequelize-mock": "^0.10.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@core(.*)$": "<rootDir>/core$1",
      "^@authModule(.*)$": "<rootDir>/modules/auth$1",
      "^@hallsModule(.*)$": "<rootDir>/modules/halls$1",
      "^@paymentsModule(.*)$": "<rootDir>/modules/payments$1",
      "^@classesModule(.*)$": "<rootDir>/modules/classes$1",
      "^@pricesModule(.*)$": "<rootDir>/modules/prices$1",
      "^@registrationsModule(.*)$": "<rootDir>/modules/registrations$1",
      "^@rolesModule(.*)$": "<rootDir>/modules/roles$1",
      "^@schedulesModule(.*)$": "<rootDir>/modules/schedules$1",
      "^@usersModule(.*)$": "<rootDir>/modules/users$1",
      "^@decorators(.*)$": "<rootDir>/core/decorators$1",
      "^@guards(.*)$": "<rootDir>/core/guards$1",
      "^@middlewares(.*)$": "<rootDir>/core/middlewares$1",
      "^@services(.*)$": "<rootDir>/core/services$1",
      "^@pipes(.*)$": "<rootDir>/core/pipes$1",
      "^@root(.*)$": "<rootDir>$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
